package sprites;

/**
 * class that represents the Dirt units '.' in this game Vacuum try to eat the
 * Dirt
 * 
 * @author gerrit
 *
 */
public class Dirt extends Sprite {
  protected int value;

  public Dirt(char symbol, int row, int column, int value) {
    super(symbol, row, column);
    this.value = value;
  }

  /**
   * returns the score value for the Dirt Sprite.
   * 
   * @return returns the score
   */
  public int getValue() {
    return value;
  }

}
